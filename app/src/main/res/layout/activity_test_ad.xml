<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <!-- 信息流广告  横版视频广告-->
    <FrameLayout
        android:id="@+id/ad_NativeExpress_fl"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <!-- 信息流广告 三图模板-->
    <FrameLayout
        android:id="@+id/ad_NativeExpress_three_pic_fl"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/ad_NativeExpress_fl" />

    <!-- 插屏广告 三图模板-->
    <FrameLayout
        android:id="@+id/ad_InteractionExpress_fl"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/ad_NativeExpress_three_pic_fl" />

    <TextView
        android:id="@+id/load_TTFullScreenVideoAd_tv"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="加载全屏广告"
        app:layout_constraintTop_toBottomOf="@id/ad_InteractionExpress_fl" />

    <TextView
        android:id="@+id/show_TTFullScreenVideoAd_tv"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="显示全屏广告"
        app:layout_constraintTop_toBottomOf="@id/load_TTFullScreenVideoAd_tv" />

    <TextView
        android:id="@+id/load_RewardVideoAd_tv"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="加载激励视频广告"
        app:layout_constraintTop_toBottomOf="@id/show_TTFullScreenVideoAd_tv" />

    <TextView
        android:id="@+id/show_RewardVideoAd_tv"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="显示激励视频广告"
        app:layout_constraintTop_toBottomOf="@id/load_RewardVideoAd_tv" />

    <TextView
        android:id="@+id/show_TTDrawFeedAd_tv"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="显示Draw信息流"
        app:layout_constraintTop_toBottomOf="@id/show_RewardVideoAd_tv" />

    <FrameLayout
        android:id="@+id/csj_ad_frame_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toBottomOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>